{
   "browser_action": {
      "default_icon": "search32.png",
      "default_popup": "search.html",
      "default_title": "VerbatimSearch"
   },
   "description": "Google verbatim search",
   "icons": {
      "32": "search32.png",
      "16": "search16.png"
